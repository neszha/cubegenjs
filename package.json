{
    "name": "cubegenjs",
    "version": "0.1.5",
    "description": "Protecting and Optimizing your JavaScript Source Code",
    "main": "index.js",
    "typings": "index.d.js",
    "type": "module",
    "engines": {
        "node": ">= 18.0.0"
    },
    "keywords": [
        "cubegen",
        "cubegenjs",
        "builder",
        "obfuscator",
        "node-protector",
        "web-protector"
    ],
    "scripts": {
        "clear:builder": "gulp clear:builder-directory",
        "build": "npm run clear:builder && npm run build:bundler && npm run build:obfuscator && npm run build:web-protector && npm run build:node-protector && npm run build:cli && npm run build:root-module",
        "build:root-module": "tsc -p tsconfig.prod.json",
        "build:watch": "tsc --watch -p tsconfig.esm.json",
        "build:cli": "lerna run build --scope=@cubegenjs/cli",
        "build:bundler": "lerna run build --scope=@cubegenjs/bundler",
        "build:obfuscator": "lerna run build --scope=@cubegenjs/obfuscator",
        "build:web-protector": "lerna run build --scope=@cubegenjs/web-protector",
        "build:node-protector": "lerna run build --scope=@cubegenjs/node-protector"
    },
    "repository": "neszha/cubegenjs",
    "author": "Fanesa Hadi Pramana",
    "license": "MIT",
    "dependencies": {
        "@cubegenjs/bundler": "^0.1.1",
        "@cubegenjs/cli": "^0.1.2",
        "@cubegenjs/node-protector": "^0.1.1",
        "@cubegenjs/obfuscator": "^0.1.1",
        "@cubegenjs/web-protector": "^0.1.4",
        "chalk": "^5.3.0",
        "commander": "^12.0.0",
        "events": "^3.3.0",
        "fs-extra": "^11.2.0",
        "randomstring": "^1.3.0"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.4",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.25",
        "@types/randomstring": "^1.3.0",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "del": "^7.1.0",
        "eslint": "^8.57.0",
        "eslint-config-standard-with-typescript": "^43.0.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-n": "^16.6.2",
        "eslint-plugin-promise": "^6.1.1",
        "gulp": "^5.0.0",
        "gulp-cli": "^3.0.0",
        "jest": "^29.7.0",
        "lerna": "^8.1.2",
        "ts-jest": "^29.1.2",
        "typescript": "^5.4.2"
    },
    "workspaces": [
        "packages/bundler",
        "packages/cli",
        "packages/obfuscator",
        "packages/web-protector",
        "packages/node-protector"
    ]
}
