{
    "name": "cubegen",
    "version": "0.1.0",
    "description": "Protecting and Optimizing your JavaScript Source Code",
    "main": "index.js",
    "typings": "index.d.js",
    "type": "module",
    "engines": {
        "node": ">= 18.0.0"
    },
    "scripts": {
        "clear:builder": "gulp clear-builder-directory",
        "build": "npm run clear:builder && npm run build:bundler && npm run build:obfuscator && npm run build:node-protector && npm run build:root-module && npm run build:npm-publish",
        "build:root-module": "tsc -p tsconfig.prod.json && tsc -p tsconfig.esm.json",
        "build:watch": "tsc --watch -p tsconfig.esm.json",
        "build:bundler": "lerna run build --scope=@cubegen/bundler",
        "build:obfuscator": "lerna run build --scope=@cubegen/obfuscator",
        "build:node-protector": "lerna run build --scope=@cubegen/node-protector",
        "build:npm-publish": "gulp build-npm-publish-directory",
        "test": "lerna run test"
    },
    "repository": "neszha/cubegen",
    "author": "Fanesa Hadi Pramana",
    "license": "MIT",
    "dependencies": {
        "@cubegen/bundler": "^0.1.0",
        "@cubegen/node-protector": "^0.1.0",
        "@cubegen/obfuscator": "^0.1.0",
        "commander": "^12.0.0",
        "fs-extra": "^11.2.0"
    },
    "devDependencies": {
        "@types/fs-extra": "^11.0.4",
        "@types/jest": "^29.5.12",
        "@types/node": "^20.11.25",
        "@typescript-eslint/eslint-plugin": "^6.21.0",
        "del": "^7.1.0",
        "eslint": "^8.57.0",
        "eslint-config-standard-with-typescript": "^43.0.1",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-n": "^16.6.2",
        "eslint-plugin-promise": "^6.1.1",
        "gulp": "^5.0.0",
        "gulp-cli": "^3.0.0",
        "jest": "^29.7.0",
        "lerna": "^8.1.2",
        "ts-jest": "^29.1.2",
        "typescript": "^5.4.2"
    },
    "bin": {
        "cubegen": "dist/esm/bin/cubegen.js"
    },
    "workspaces": [
        "packages/bundler",
        "packages/obfuscator",
        "packages/node-protector"
    ]
}
